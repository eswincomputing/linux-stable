			reset-names = "axi", "cfg", "isp0", "isp1", "dvp", "sht0", "sht1", "sht2", "sht3", "sht4", "sht5";

			id = <0>;
			#size-cells = <2>;
			reg = <0x0 0x51030000 0x0 0x10000>;
		};

