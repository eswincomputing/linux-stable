config ESWIN_DSP
    tristate "Eswin DSP"
    default n

config DSP_PERF
    bool "DSP PERF"
    default n
    depends on ESWIN_DSP

config ESWIN_NPU
    tristate "Eswin NPU"
    default n
    depends on ESWIN_DSP
    select ESWIN_DEV_DMA_BUF
    select ESWIN_IOMMU_RSV
    select ESWIN_RSVMEM_HEAP

config NPU_PERF
    bool "NPU PERF"
    default n
    depends on ESWIN_NPU